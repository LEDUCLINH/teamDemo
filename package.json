{
  "name": "1stkare-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "dev": "next dev",
    "analyze": "cross-env ANALYZE=true next build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && prettier --check pages src",
    "format": "next lint --fix && prettier --check --write pages src",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "aws-sdk": "2.984.0",
    "axios": "^0.24.0",
    "dayjs": "1.10.6",
    "firebase": "8.10.0",
    "firebase-admin": "9.11.1",
    "graphql": "15.5.3",
    "graphql-request": "3.5.0",
    "graphql-tag": "2.12.5",
    "material-ui-dropzone": "3.5.0",
    "micro": "9.3.4",
    "multer": "1.4.3",
    "multer-sharp-s3": "0.2.1",
    "next": "11.1.2",
    "next-connect": "0.10.2",
    "next-seo": "4.26.0",
    "nodemailer": "6.6.3",
    "nodemailer-mailgun-transport": "2.1.3",
    "nprogress": "0.2.0",
    "qs": "6.10.1",
    "react": "17.0.2",
    "react-autosuggest": "^10.1.0",
    "react-dom": "17.0.2",
    "stripe": "8.174.0",
    "subscriptions-transport-ws": "0.9.19",
    "urql": "2.0.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "2.2.1",
    "@graphql-codegen/typescript-operations": "2.1.3",
    "@graphql-codegen/typescript-urql": "3.1.3",
    "@next/bundle-analyzer": "11.1.2",
    "@types/micro": "7.3.6",
    "@types/multer": "1.4.7",
    "@types/nodemailer": "6.4.4",
    "@types/nodemailer-mailgun-transport": "1.4.3",
    "@types/nprogress": "0.2.0",
    "@types/qs": "6.9.7",
    "@types/react": "17.0.20",
    "@types/react-autosuggest": "^10.1.5",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.2",
    "prettier": "2.3.2",
    "sass": "1.39.0",
    "typescript": "4.4.2"
  },
  "prettier": {
    "endOfLine": "auto",
    "semi": false,
    "singleQuote": true
  }
}
